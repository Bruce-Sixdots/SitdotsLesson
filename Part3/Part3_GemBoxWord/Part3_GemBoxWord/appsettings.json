{
  "Logging": {
    "LogLevel": {
      "Default": "Information", // 預設的日誌級別設為 Information
      "Microsoft.AspNetCore": "Warning" // 只顯示 Microsoft.AspNetCore 相關的 Warning 訊息
    }
  },
  "ConnectionStrings": {
    "SQLiteConnection": "Data Source=./Data/Demo.db" // 設定 SQLite 資料庫的連接字串
  },
  "AllowedHosts": "*", // 允許所有主機請求
  "AppSettings": {
    "SomeKey": "SomeValue" // 可自訂應用程式設定
  }
}
